        /*  ===== MEDIA QUERIES ======  */
        @media screen and (max-width: 768px) {

            /*  ===== BASE ======  */
            .container {
                padding: 0.8rem 0.5rem;
            }

            /*  ===== HEADER ======  */
            header {
                padding: 0.8rem 0.5rem;
            }

            .menu-icons {
                display: block;
            }

            .nav-bar {
                background-color: var(--orange-color);
                border-top-left-radius: 5px;
                width: 80%;
                height: 100vh;
                position: absolute;
                top: 4.5rem;
                right: 0;
                transform: scalex(0);
                transform-origin: right;
                transition: transform 300ms;
                z-index: 1;
            }

            .show-menu {
                transform: scalex(1);
            }

            .nav-items {
                display: flex;
                flex-direction: column;
                margin-top: 1.5rem;
            }

            .nav-item {
                margin: 0.8rem 2rem;
            }

            .nav-link {
                background-image: none;
                color: var(--primary-clr);
                font-weight: 500;
                font-size: 1.3rem;
            }

            .nav-link::before {
                background-color: var(--primary-clr);
            }

            /*  ===== BODY ========  */
            .home_img {
                display: none;
            }
        }

        @media screen and (min-width: 481px) {
            header {
                padding: 1.2rem 1.5rem;
            }

            /*  ===== BODY ========  */
            .slogan {
                padding: 0 1.5rem;
            }

            .slogan .slogan-title {
                font-size: 2.2rem;
                max-width: 38rem;
            }

            .slogan .slogan-text {
                font-size: 1.15rem;
                margin-top: 1.3rem;
                max-width: 30rem;
                line-height: 1.5rem;
            }

            .hero-btn {
                width: 300px;
                padding: 0.8rem 0;
                border-radius: 5rem;
                margin-top: 3rem;
            }

            .home-btn-link {
                font-size: 1.3rem;
            }

            /* services */
            .services-container {
                column-gap: 1rem;
            }
        }

        @media screen and (min-width: 769px) {
            header {
                padding: 1.2rem 5rem;
            }

            /*  ===== BODY ========  */
            .section_1 {
                padding: 0 5rem;
                height: 80vh;
            }

            .slogan {
                margin-top: 8rem;
            }

            .slogan .slogan-title {
                font-size: 3rem;
            }

            .hero-btn {
                width: 280px;
                margin-top: 1.8rem;
            }

            .home-btn-link {
                font-size: 1.2rem;
            }

            /* - About - */
            .section_2 {
                height: 100vh;
                background-color: var(--primary-clr);
                position: relative;
            }

            .about {
                background: linear-gradient(-45deg,
                        hsla(0, 20%, 4%, 0.1),
                        hsla(0, 42%, 6%, 0.3));
                backdrop-filter: blur(5px);
                -webkit-backdrop-filter: blur(5px);
                border-radius: 10px;
                border: thin solid rgb(239, 239, 239, 0.6);
                padding: 2rem 1.5rem;
                position: absolute;
                top: 50%;
                transform: translateY(-50%);
                left: 20rem;
                z-index: 1;
            }

            .about-container img {
                display: block;
                position: absolute;
                max-width: 50%;
                top: 50%;
                transform: translateY(-50%);
                right: 0;
            }

            .about .about-title {
                font-size: 1.8rem;
                margin-bottom: 1.2rem;
            }

            .feature_desc {
                font-size: 1.05rem;
                letter-spacing: 0.05rem;
                opacity: 0.8;
                max-width: 25rem;
                line-height: 1.3rem;
            }

            /* section 3 */
            .services {
                margin: 3rem 0;
            }

            .service-title {
                margin-top: 2rem;
                font-size: 1.1rem;
            }

            .service span {
                top: 1.5rem;
                left: 1.3rem;
            }

            .service-description {
                font-size: 0.9rem;
            }

            .services-container {
                row-gap: 5rem;
                column-gap: 1rem;
                place-items: end;
                grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            }

            /* section 4 */
            .section_4 {
                display: block;
                margin: 3rem 0;
            }

            .glide_slides {
                display: grid;
                grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            }

            .babysitter {
                height: 80%;
                margin-right: 1.5rem;
            }

            .babysitter img {
                width: 25%;
            }

            .baby-sitter-name {
                font-size: 1.4rem;
                font-weight: 400;
            }

            .baby-sitter-story {
                font-size: 0.95rem;
                opacity: 0.8;
                margin: 0rem 0 0.2rem 0;
                line-height: 1.5rem;
            }

            /* section 5 */
            .section_5 {
                margin: 2rem 0;
            }

            .testimonials-container {
                display: grid;
                gap: 2rem;
                grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            }

            .testimony {
                margin: 0;
            }

            .testimony img {
                width: 20%;
            }

            .tester-name {
                font-size: 1.4rem;
                font-weight: 400;
            }

            .testimony-text {
                font-size: 0.91rem;
                opacity: 0.8;
            }

            /* footer */
            .top-footer {
                flex-direction: row;
                justify-content: space-between;
            }

            .address-text,
            .phone-text,
            .email-text {
                font-size: 0.9rem;
            }
        }

